{
  "name": "@jamjar/api",
  "version": "1.0.0",
  "description": "Workers are nodejs threads that run tasks for a given jam.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=TEST jest",
    "start": "npm run build && node ./dist/index.js",
    "start:watch": "onchange -d 300 -i -k \"./dist/**/*\" -- node ./dist/index.js",
    "dev": "concurrently -n build,format,PROC npm:build:watch npm:prettier:watch npm:start:watch",
    "build:watch": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts",
    "prettier:watch": "onchange -d 10000 \"**/*.ts\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "Nick Palenchar <nickpal@nickpalenchar.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "onchange": "^7.1.0",
    "prettier": "3.1.1",
    "tsup": "^8.0.1"
  },
  "dependencies": {
    "@jamjar/database": "^1.0.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "fp-ts": "^2.16.1",
    "io-ts": "^2.2.21"
  }
}
